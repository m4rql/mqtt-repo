<div class="row"> 
  <div class="col-sm">
    <div class="card card-body mb-2 col-sm" style="background-color: #FACC2E;">
        <h4 align="center">HUMIDITY &nbsp<i class="fas fa-tint"></i></h4>
        <div class="card card-body mb-2">
          <h6 align="center">Latest values received</h6>
          <div id="latest-hum"></div>
          <hr/>
        </div>
        <div class="card card-body mb-2">
          <h6 align="center">Average</h6>
          <div id="avg-hum"></div>
          <hr/>
        </div>
        <div class="card card-body mb-2">
          <h6 align="center">Values received during last hour</h6>
            <div class="card card-body mb-2">
              <div id="lasthour-hum">
                {{#each values}}
                  <c id="{{this.date}}"><font size=2><{{this.humidity}} %></font></c>
                {{/each}}
              </div>
            </div>
        </div>
      </div>
    </div>
    <div class="col-sm">
      <div class="card card-body mb-2 col-sm">
        <h4 align="center">ANALYTICS &nbsp<i class="fas fa-chart-bar"></i></h4>
        <canvas id="hum-chart"></canvas>
        <canvas id="hum-chart-avg"></canvas>
      </div>
    </div>
  </div>
